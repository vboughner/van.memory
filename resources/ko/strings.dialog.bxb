template-macro-def (LIST_MEMORIES) {
  params {
    param (success) {
      type (core.Boolean)
      min (Required)
      max(One)
    }
    param (memories) {
      type (Memory)
      min (Optional)
      max (Many)
    }
  }
  content {
    if (success) {
      if (size(memories) == 0) {
        template ("저장된 기억이 없습니다.")
      } else-if (size(memories) == 1) {
        template ("기억 1 개 있습니다.")
      } else {
        template ("#{size (memories)} 개의 기억가 있습니다.")
      }
    } else {
      template("문제가 발생하여 기억을 검색 할 수 없습니다.")
    }
  }
}

